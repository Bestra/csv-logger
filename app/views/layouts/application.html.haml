!!!
%html
  %head
    %title CsvLogger
    = stylesheet_link_tag    "application", :media => "all"
    = javascript_include_tag "application"
    = csrf_meta_tags
  %body
    %p.notice= notice
    %p.alert= alert
    -if user_signed_in?
      = link_to 'Sign out' , destroy_user_session_path, method: :delete
    = yield
